#
# Makefile for the sensors drivers.
#

# Each configuration option enables a list of files.

obj-$(CONFIG_SENSORS_SYMLINK_CORE)	+= sensors_core.o
obj-$(CONFIG_SENSORS_K2DH)	+= k2dh.o
obj-$(CONFIG_SENSORS_AK8963C)	+= ak8963.o
obj-$(CONFIG_SENSORS_GP2A_PROXIMITY)	+= gp2a_proximity.o
obj-$(CONFIG_INPUT_MPU6500)	+= mpu6500/
obj-$(CONFIG_SENSORS_GP2A0X0)	+= gp2a0x0.o
obj-$(CONFIG_SENSORS_ALPS)	+= alps/
obj-$(CONFIG_SENSORS_GRIP_SX9500)		+= sx9500.o
obj-$(CONFIG_SENSORS_ACCELOMETER_BMA25X)	+= bma255.o
